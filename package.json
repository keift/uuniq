{
  "name": "uuniq",
  "version": "1.2.0",
  "description": "Unique Snowflake IDs and Symbolic IDs that can be generated for thousands of years.",
  "license": "MIT",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev:snowflake": "bun run --watch ./examples/snowflake.ts",
    "dev:symbolic": "bun run --watch ./examples/symbolic.ts",
    "test": "bun run build && npm run test:snowflake && npm run test:symbolic",
    "test:snowflake": "npm run ./test/snowflake.ts",
    "test:symbolic": "npm run ./test/symbolic.ts",
    "build": "tsup ./main.ts --dts --clean --format esm,cjs --out-dir ./dist"
  },
  "dependencies": {
    "@types/any-base": "^1.1.3",
    "any-base": "^1.1.0"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keift/uuniq.git"
  },
  "keywords": [
    "id",
    "unique",
    "unique-id",
    "snowflake",
    "snowflake-id",
    "symbolic",
    "symbolic-id"
  ]
}